{
  "name": "styles",
  "version": "0.1.0",
  "description": "Zeplin extension for Styles framework. (https://github.com/inloop/Styles)",
  "scripts": {
    "start": "concurrently \"zem start\" \"tsc ./sources/*.ts --watch --outDir ./src --allowJs --target \"es5\"\"",
    "build": "tsc && zem build",
    "clean": "zem clean",
    "exec": "zem exec"
  },
  "zeplin": {
    "displayName": "Styles",
    "projectTypes": [
      "ios"
    ]
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "mout": "^1.1.0",
    "zem": "^0.1.2"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^4.0.1",
    "source-map-loader": "^0.2.3",
    "typescript": "^2.7.2"
  }
}
